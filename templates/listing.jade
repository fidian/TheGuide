extends layout

block content
	!= page.html
	h2 Subpage Listing
	- var siblings = _.chain(page.parent).filter(function (item) {
	-    return item !== page && item.metadata && item.metadata.title;
	- }).sortBy(function (item) {
	-    return item.metadata.title.toLowerCase();
	- }).value();
	each sibling in siblings
		li
			a(href="/"+sibling.getFilename())= sibling.metadata.title
